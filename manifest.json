{
  "manifest_version": 2,
  "name": "Focal",
  "description": "A Chrome extension to boost your productivity",
  "version": "0.0.0.2",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Focal"
  },
  "chrome_url_overrides" : {
    "newtab" : "main.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "history",
    "tabs",
    "*://*/*",
    "storage",
    "unlimitedStorage",
    "identity",
    "notifications"
  ],
  "icons": {
    "128": "icon.png"
  },
  "oauth2": {
    "client_id": "787306999678-fsjshgsfgl2c4qhbnkocknas8c8r4pi9.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "key": ""
}
