{
  "manifest_version": 2,
  "name": "RCR",
  "description": "React, Chrome, and Redux example",
  "version": "0.0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "RCR Example",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides" : {
    "newtab" : "main.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "history",
    "tabs",
    "*://*/*",
    "storage",
    "unlimitedStorage",
    "identity"
  ],
  "oauth2": {
    "client_id": "787306999678-fsjshgsfgl2c4qhbnkocknas8c8r4pi9.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "key": ""
}
