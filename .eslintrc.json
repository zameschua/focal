{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": "off",
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "import/prefer-default-export": "off",
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "chrome":true
  }
}
